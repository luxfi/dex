    "test-prices": "ts-node test/test-prices.ts"
  },
  "dependencies": {
    "@pythnetwork/client": "^2.19.0",
    "@pythnetwork/hermes-client": "^1.0.0",
    "axios": "^1.6.2",
    "ws": "^8.15.1",
    "decimal.js": "^10.4.3",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}